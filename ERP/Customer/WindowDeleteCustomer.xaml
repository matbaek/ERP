        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ERP"
        mc:Ignorable="d"
       Title="WindowDeleteCustomer" Height="120" Width="310"
       WindowStartupLocation="CenterScreen">
    <Grid>
        <Label x:Name="LabelDeleteCustomer" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="282" Height="29"/>
        <Button x:Name="ButtonYes" Content="Ja" HorizontalAlignment="Left" Margin="50,49,0,0" VerticalAlignment="Top" Width="75" Click="ButtonYes_Click"/>
        <Button x:Name="ButtonNo" Content="Nej" HorizontalAlignment="Left" Margin="167,49,0,0" VerticalAlignment="Top" Width="75" Click="ButtonNo_Click"/>
    </Grid>
</Window>
